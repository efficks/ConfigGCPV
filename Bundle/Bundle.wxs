<Wix xmlns="http://wixtoolset.org/schemas/v4/wxs" xmlns:bal="http://wixtoolset.org/schemas/v4/wxs/bal"
     xmlns:util="http://wixtoolset.org/schemas/v4/wxs/util">
  <Bundle Name="Bundle" Manufacturer="TODO Manufacturer" Version="1.0.0.0" UpgradeCode="ed07a628-bfc4-4668-ab0f-bcd1990d7c68">
    <BootstrapperApplication>
      <bal:WixStandardBootstrapperApplication LicenseUrl="https://www.example.com/license" Theme="hyperlinkLicense" />
    </BootstrapperApplication>

    <util:ProductSearch ProductCode="{90140000-00D1-0409-1000-0000000FF1CE}" Variable="ACCESS_REDIST_PRESENT" Result="state" />

    <Chain>
      <!-- TODO: Define the list of chained packages. -->
      <!-- <MsiPackage SourceFile="path\to\your.msi" /> -->
      <MsiPackage SourceFile="$(var.Setup.TargetPath)" />
      <ExePackage SourceFile="$(var.SolutionDir)thirdparty\AccessDatabaseEngine_X64.exe"
                  DownloadUrl="https://download.microsoft.com/download/2/4/3/24375141-E08D-4803-AB0E-10F2E3A07AAA/AccessDatabaseEngine_X64.exe"
                  InstallArguments="/quiet"
                  Permanent="yes"
                  PerMachine="yes"
                  DetectCondition="ACCESS_REDIST_PRESENT&lt;&gt;5"/>
    </Chain>

  </Bundle>
</Wix>
